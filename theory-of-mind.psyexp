        <Param name="Begin Routine" updates="constant" val="&amp;#10;slide = make_slide(f&quot;slide-00-title&quot;)&amp;#10;slide.autoDraw = True&amp;#10;&amp;#10;NEXT_BUTTON.pos= (0, -0.2)&amp;#10;NEXT_BUTTON.autoDraw = True&amp;#10;&amp;#10;if USE_AUDIO:&amp;#10;    aud_file = f&quot;{AUD_DIR}/slide-00-title.m4a&quot;&amp;#10;    SOUND = make_sound(&quot;slide-00-title&quot;, aud_file)&amp;#10;    SOUND_DUR = SOUND.getDuration()&amp;#10;    SOUND_T = 0&amp;#10;    SOUND.play()&amp;#10;" valType="extendedCode"/>
